{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/dashboard(.*)",
      "dependency": "dashboard",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "dependency": "frontend",
      "target": "$1",
      "localDir": "../frontend/dist",
      "authenticationType": "xsuaa"
    },
    {
      "dependency": "cap-server",
      "authenticationType": "xsuaa"
    }
  ]
}
