{
  "welcomeFile": "index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/dashboard(.*)",
      "dependency": "spaceapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "dependency": "frontend",
      "target": "$1",
      "localDir": "../frontend/out",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/mainapp/dist(.*)$",
      "dependency": "mainapp",
      "target": "$1",
      "destination": "*",
      "authenticationType": "xsuaa"
    },
    {
      "dependency": "cap-server",
      "authenticationType": "xsuaa"
    }
  ]
}
