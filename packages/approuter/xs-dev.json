{
  "welcomeFile": "index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/dashboard(.*)",
      "dependency": "dashboard",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/frontend/dist(.*)$",
      "dependency": "frontend",
      "target": "$1",
      "destination": "*",
      "authenticationType": "xsuaa"
    },
    {
      "dependency": "cap-server",
      "authenticationType": "xsuaa"
    }
  ]
}
