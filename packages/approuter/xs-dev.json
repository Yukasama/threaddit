{
  "welcomeFile": "mainapp/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "dependency": "spaceapp",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)",
      "dependency": "frontend",
      "authenticationType": "xsuaa",
      "localDir": "../"
    },
    {
      "source": "^/(.*)",
      "dependency": "mainapp",
      "authenticationType": "xsuaa",
      "localDir": "../mainapp",
      "replace": {
        "pathSuffixes": ["index.html", ".js"],
        "vars": ["escaped_text", "NOT_ESCAPED"]
      }
    },
    {
      "dependency": "cap-server",
      "authenticationType": "xsuaa"
    }
  ]
}
